package com.microservice.leadassessment.productservice.entity;

import lombok.*;

import jakarta.persistence.*;

@Getter
@Setter
@NoArgsConstructor
@AllArgsConstructor
@Entity
@Table(name = "product")
public class Product {
	
    private String name;
    private String brand;
    private String description;
    
    @EmbeddedId
    private Attributes attributes;
    
    
}
