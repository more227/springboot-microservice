package com.microservice.leadassessment.productservice.entity;



import lombok.*;

import jakarta.persistence.*;

@Getter
@Setter
@NoArgsConstructor
@AllArgsConstructor
@Entity
@Table(name = "attributes")
@Embeddable
public class Attributes {
    
	@Id
	private Long Id;
    private String name;
    private String value;
   
}